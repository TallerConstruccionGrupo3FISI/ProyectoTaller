<!DOCTYPE html>
<html lang="en">
<% include ./partials/headers %>
<body>
  <% include ./partials/navbar %>

  <div class="container" >
    <div class="row">
    <% include ./partials/dashboard-secretaria %>
		  <div class="col-md-9">
		    <div class="card">
		        <div class="card-body">

              <div class="row">
                  <div class="col-md-12">
                      <h4>HORARIO ATENCION</h4>
                      <hr>
                  </div>
              </div>

              <div class="row">
                  <div class="col-md-12">
                    <form class="form-inline" action="/horarios" method="post">



                    <div class="form-group row mr-sm-2">
                            <label for="horaInicio" class="col-4 col-form-label ">Tiempo inicio</label>
                            <div class="col-8">
                              <input class="timepicker form-control" name="horaInicio" id="horaInicio" placeholder="Ingresar Hora inicio"/>
                            </div>
                    </div>

                    <div class="form-group row mr-sm-2">
                            <label for="horaSalida" class="col-4 col-form-label">Tiempo Salida</label>
                            <div class="col-8">
                              <input class="timepicker form-control" name="horaSalida" id="horaSalida" placeholder="Ingresar Hora Salida" width="276"/>
                            </div>
                    </div>
                  </form>
                  </div>
            </div>
        </div>
      </div>
		</div>
</div>

  <script src="js/utilidades.js"></script>
  <script>
         $('#horaInicio').timepicker();
         $('#horaSalida').timepicker();
  </script>
